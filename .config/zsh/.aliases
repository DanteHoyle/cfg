alias cfg="git --git-dir=$HOME/.cfg --work-tree=$HOME"
alias cfa="${EDITOR} ${ZDOTDIR}/.aliases"
alias cfp="${EDITOR} ${ZDOTDIR}/.profile"
alias cfr="${EDITOR} ${ZDOTDIR}/.zshrc"
